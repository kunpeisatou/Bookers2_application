<main>
  <div class='col-md-12'>
    <% if @model == 'user' %>
      <h2>Users search for "<%= @content %>"</h2>
      <%= render 'users/search', users: @records %>
    <% else @model == 'book' %>
      <h2>Books search for "<%= @content %>"</h2>
      <%= render 'books/search', books: @records %>
    <% end %>
  </div>
</main>